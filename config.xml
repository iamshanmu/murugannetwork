<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.murugannetworks" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>MURUGAN NETWORKS</name>
    <description>
        MOBILE APPLICATION 
    </description>
    <author email="admin@s22.in" href="http://s22.in">
        S22 CREATIVE STUDIO
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="android-build-tool" value="gradle" />
    <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.3" />
    <plugin name="phonegap-plugin-push" source="npm" spec="~1.8.0">
        <variable name="SENDER_ID" value="375160833480" />
    </plugin>
    <plugin name="cordova-plugin-console" source="npm" spec="~1.0.3" />
    <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.2.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <platform name="android">
       <preference name="android-minSdkVersion" value="19" />
       <preference name="android-targetSdkVersion" value="26" />
       <preference name="android-debuggable" value="false" />
        <icon density="ldpi" src="www/mn/images/icon.png" />
        <icon density="mdpi" src="www/mn/images/icon.png" />
        <icon density="hdpi" src="www/mn/images/icon.png" />
        <icon density="xhdpi" src="www/mn/images/icon.png" />
        <icon density="xxhdpi" src="www/mn/images/icon.png"/>
        <icon density="xxxhdpi" src="www/mn/images/icon.png" />
        <splash density="land-ldpi" src="www/mn/images/icon.png" />
        <splash density="land-mdpi" src="www/mn/images/icon.png" />
        <splash density="land-hdpi" src="www/mn/images/icon.png" />
        <splash density="land-xhdpi" src="www/mn/images/icon.png" />
        <splash density="land-xxhdpi" src="www/mn/images/icon.png" />
        <splash density="land-xxxhdpi" src="www/mn/images/icon.png" />
        <splash density="port-ldpi" src="www/mn/images/icon.png" />
        <splash density="port-mdpi" src="www/mn/images/icon.png"/>
        <splash density="port-hdpi" src="www/mn/images/icon.png"/>
        <splash density="port-xhdpi" src="www/mn/images/icon.png"/>
        <splash density="port-xxhdpi" src="www/mn/images/icon.png" />
        <splash density="port-xxxhdpi" src="www/mn/images/icon.png"/>
        
    </platform>
     <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
        <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
